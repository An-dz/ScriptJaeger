{
	"name": "ScriptJäger",
	"version": "0.5.1",
	"author": "André Zanghelini (An_dz)",
	"description": "Die Erweiterung für den Jägermeister! __MSG_sjDescription__",
	"homepage_url": "https://github.com/An-dz/ScriptJaeger",
	"applications": {
		"gecko": {
			"id": "scriptjaeger@zanghelini.com",
			"strict_min_version": "58.0"
		}
	},

	"icons": {
		"128": "images/jaegerhut128.png",
		"48": "images/jaegerhut48.png",
		"16": "images/jaegerhut16.png"
	},
	"options_ui": {
		"page": "prefs.html",
		"open_in_tab": true,
		"browser_style": false,
		"chrome_style": false
	},

	"background": {
		"scripts": [
			"background.js"
		]
	},

	"browser_action": {
		"default_icon": {
			"19": "images/undefined19.png",
			"38": "images/undefined38.png"
		},
		"default_title": "ScriptJäger",
		"default_popup": "popup.html"
	},

	"web_accessible_resources": [
		"prefs.html"
	],

	"permissions": [
		"alarms",
		"notifications",
		"storage",
		"tabs",
		"webNavigation",
		"webRequest",
		"webRequestBlocking",
		"<all_urls>"
	],

	"default_locale": "en",

	"manifest_version": 2
}
